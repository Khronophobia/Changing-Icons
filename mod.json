{
	"geode": "3.0.0-alpha.2",
	"gd": {
		"win": "2.204",
		"android": "2.205"
	},
	"version": "v1.0.0",
	"id": "khronophobia.changing_icons",
	"name": "Changing Icons",
	"developer": "Khronophobia",
	"description": "Randomly change icons during gameplay",
	"repository": "https://github.com/Khronophobia/Changing-Icons",
	"resources": {
		"sprites": [
			"resources/*.png"
		]
	},
	"tags": ["customization", "gameplay", "interface"],
	"dependencies": [
		{
			"id": "geode.node-ids",
			"importance": "required",
			"version": ">=v1.2.0"
		}
	],
	"settings": {
		"disable-locked-icons": {
			"name": "Disable Locked Icons",
			"description": "Prevents locked icons and colors from being added to the list. Also prevents locked icons from being picked when <cy>Use All Icons</c> is enabled. Does not affect existing lists",
			"type": "bool",
			"default": true
		},
		"delete-confirmation": {
			"name": "Show Delete Confirmations",
			"description": "Show delete confirmations when removing icons from a list",
			"type": "bool",
			"default": true
		}
	},
	"incompatibilities": [
		{
			"id": "rooot.custom-gamemode-colors",
			"importance": "conflicting",
			"version": ">=0.1.1"
		},
		{
			"id": "TheSillyDoggo.Cheats",
			"importance": "conflicting",
			"version": ">=1.0.0"
		},
		{
			"id": "weebify.separate_dual_icons",
			"importance": "conflicting",
			"version": ">=v1.0.0"
		}
	]
}